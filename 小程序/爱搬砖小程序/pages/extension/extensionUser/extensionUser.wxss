/* pages/extension/extensionUser/extensionUser.wxss */
.main{
}
page{
	background: #FFFFFF;
}
.main-header{
	width: 100%;
	height: 44rpx;
	background-color: #ec5c71;
	color: #FFFFFF;
	line-height: 44rpx;
	font-size: 25rpx;
	letter-spacing: 5rpx;
	padding-left: 50rpx;
	box-sizing: border-box;
}
.userList {
	display: flex;
	flex-wrap: wrap;
	padding: 25rpx;
	box-sizing: border-box;
}
.user {
	width: 25%;
	width: 25%;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.user image {
	width: 50rpx;
	height: 50rpx;
	border-radius: 50%;
	position: relative;
	top: 10rpx;
}
.userName{
	font-size: 25rpx;
	color: #656564;
	letter-spacing: 3rpx;
	line-height: 30rpx;
	position: relative;
	top: -5rpx;
	margin-left: 10rpx;
}


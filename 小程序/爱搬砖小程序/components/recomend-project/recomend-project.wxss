/* components/recomend-project/recomend-project.wxss */
.recomend-project{
  height: 448rpx;
  width: 100%;
	background-color: #f5f6f8;
	border-radius: 10rpx;
  padding: 10rpx;
  box-sizing: border-box;
  /* margin-bottom: 20rpx; */
  margin-top: 17rpx;
}
.project-img{
  width: 670rpx;
	height: 280rpx;
  margin: auto;
  margin-bottom: 18rpx;
}
.project-img image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.recomend-title{
  font-family: MicrosoftYaHei;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 40rpx;
	letter-spacing: 0rpx;
	color: #333333;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.recomend-intr{
  font-family: MicrosoftYaHei;
	font-size: 20rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 28rpx;
	letter-spacing: 2rpx;
	color: #999999;
  overflow: hidden;
  text-overflow:ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-top: 24rpx;
}
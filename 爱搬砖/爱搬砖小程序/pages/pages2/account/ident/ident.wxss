/* pages/pages2/account/ident/ident.wxss */

.main{
  padding: 30rpx;
  box-sizing: border-box;
}
.forms{
  background: #ffffff;
  padding: 20rpx;
  box-sizing: border-box;
  box-shadow: 0rpx 6rpx 15rpx 0rpx 
		rgba(44, 133, 255, 0.15);
	border-radius: 20rpx;
}
.lis{
  height: 130rpx;
  border-bottom: 1rpx dashed #e5e5e5;
  font-size: 30rpx;
  color: #555555;
  letter-spacing: 2rpx;
  line-height: 130rpx;
}
.titles{
  float: left
}
.check{
  float: right;
  display: flex;
}
.check-li{
  display: flex
}
.wai{
  display: block;
  position: relative;
  background: #ffffff;
  border: solid 1rpx #d9d9d9;
  border-radius: 50%;
  z-index: 7;
  width: 30rpx;
  height: 30rpx;
  top: 50rpx;
  margin-right: 10rpx;
}
.nei{
  display: block;
  width: 18rpx;
  height: 18rpx;
  background: #999;
  z-index: 9;
  border-radius:50%; 
  margin-top: 6.5rpx;
  margin-left: 6.5rpx;
  opacity: 0.6;
}
.nei-active{
  background: #3388ff;
}
.margin{
  margin-right: 20rpx;
}

.li{
  height: 130rpx;
  border-bottom: 1rpx dashed #e5e5e5;
  line-height: 130rpx;
  font-size: 30rpx;
  color: #555555;
  position: relative;
}
.li input{
  height: 100%;
  font-size: 30rpx;
}
.title{
  float: left;
  margin-right: 25rpx;
  letter-spacing: 2rpx;
}

.content{
  font-family: MicrosoftYaHei;
	font-size: 22rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: 1rpx;
	color: #cccccc;
  margin-top: 20rpx;
  white-space: pre-line;
  padding-left: 15rpx;
}

.yingye{
  height:250rpx;
  border-bottom: 1rpx dashed #e5e5e5;
  line-height: 130rpx;
  font-size: 30rpx;
  color: #555555;
  position: relative;
  display: flex;
}
.tianjia{
  width: 150rpx;
	height: 150rpx;
	background-color: #ffffff;
	border-radius: 6rpx;
	border: dashed 1rpx #bababa;
  color: #333;
  text-align: center;
  margin-top: 40rpx;
  margin-left: 30rpx;
}
.tianjias{
  width: 150rpx;
	height: 150rpx;
	background-color: #ffffff;
	border-radius: 6rpx;
	/* border: dashed 1rpx #bababa; */
  color: #333;
  text-align: center;
  margin-top: 40rpx;
  margin-left: 30rpx;
}
.jia{
  font-size: 85rpx;
  display: block;
  height: 150rpx;
  position: relative;
  top: 5rpx;
  font-weight: normal
}
.tianjias image{
  width: 100%;
  height: 100%;
}
.biao{
  display: block;
  width: 150rpx;
}
.none{
  border: none;
}
/* pages/pages2/userSelect/gerenForm/gerenForm.wxss */
.main{
  padding: 30rpx;
  box-sizing: border-box;
}
.forms{
  background-color: #ffffff;
	box-shadow: 0rpx 6rpx 15rpx 0rpx 
		rgba(44, 133, 255, 0.15);
	border-radius: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.li{
  height: 130rpx;
  border-bottom: 1rpx dashed #e5e5e5;
  line-height: 130rpx;
  font-size: 30rpx;
  color: #555555;
  position: relative;
}
.li input{
  height: 100%;
  font-size: 30rpx;
}
.left{
  float: left
}
.title{
  margin-right: 25rpx;
  letter-spacing: 2rpx;
}
.ident-img{
  height: 540rpx;
  border-bottom: 1rpx dashed #e5e5e5;
  line-height: 130rpx;
}
.ident-main{
  height: 326rpx;
  border-radius: 5rpx;
	border: solid 1rpx #eaeaea;
}
.ident-main image{
  width: 100%;
  height: 100%;
}
.intr{
  font-size: 21rpx;
  color: #cccccc;
  letter-spacing: 2rpx;
  line-height: 50rpx;
}
.none{
  border: none;
}
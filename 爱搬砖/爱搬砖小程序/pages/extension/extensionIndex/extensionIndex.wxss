/* pages/extension/extensionIndex/extensionIndex.wxss */
.main{
  background: #f34353;
  padding-bottom: 193rpx;
  box-sizing: border-box;
  position: relative;
}
.header{
	height: 731rpx;
}
.header image {
	width: 100%;
	height: 100%;
}
.messages{
	width:686rpx ;
	height: 200rpx;
	padding-top: 60rpx;
	box-sizing: border-box;
	margin: auto;
	position: relative;
}
.tiao{
	width: 686rpx;
	height: 25rpx;
	background-color: #7c0013;
	box-shadow: inset 0px 3rpx 3rpx 0rpx 
		rgba(4, 0, 0, 0.34);
	border-style: solid;
	border-width: 1px;
	border-image-source: linear-gradient(0deg, 
		rgba(255, 255, 255, 0.28) 0%, 
		#7c0013 100%);
	border-image-slice: 1;
	position: relative;
}
.start{
	background: url(data:image/png;base64,);
	background-repeat: repeat;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	width: 0%;
}
.twoHand{
	position: absolute;
	top: -38rpx;
	left: 23%;
}
.numbers {
	color: #FFFFFF;
	font-size: 20rpx;
	letter-spacing: 1rpx;
	display: block;
	position: relative;
	left: -10rpx;
}
.numbers2 {
	color: #FFFFFF;
	font-size: 20rpx;
	letter-spacing: 1rpx;
	display: block;
	position: relative;
	left: -10rpx;
}
.fiveHand {
	position: absolute;
	top: -38rpx;
	left: 48%;
}
.endHand {
	position: absolute;
	top: -38rpx;
	right: -10rpx;
	text-align: right;
}
.endHand image {
	width: 39rpx;
	height: 38rpx;
	
}
.fiveHand image {
	width: 39rpx;
	height: 38rpx;
}
.twoHand image {
	width: 39rpx;
	height: 38rpx;
}

.triangle{
	margin-top: 10rpx;
	position: absolute;
	left:0;
}
.triangle image {
	width: 34rpx;
	height: 38rpx;
	position: relative;
	left: -14rpx;
}
.acNumber{
	font-size: 20rpx;
	color: #FFFFFF;
	position: relative;
	left: -20%;
}
.acText {
	color: #ffc62f;
}
.pickInvi{
	width: 575rpx;
	height: 72rpx;
	background-color: #ffc62f;
	box-shadow: 0px 6rpx 0rpx 0rpx 
		#d79c00, 
		0px -1rpx 0rpx 0rpx 
		rgba(255, 255, 255, 0.5);
	border-radius: 36rpx;
	text-align: center;
	margin: auto;
	font-size: 34rpx;
	color: #FFFFFF;
	letter-spacing: 3rpx;
	line-height: 72rpx;
	font-weight: 600;
	margin-top: 20rpx;
	margin-bottom: 30rpx;
}
.Invitation button{
	border: none;
	color: #FFFFFF;
	background-color: #ffc62f;
	box-shadow: 0px 6rpx 0rpx 0rpx 
		#d79c00, 
		0px -1rpx 0rpx 0rpx 
		rgba(255, 255, 255, 0.5);
}
.Invitation button:hover {
	border: none;
	color: #FFFFFF;
	background-color: #ffc62f;
	box-shadow: 0px 6rpx 0rpx 0rpx 
		#d79c00, 
		0px -1rpx 0rpx 0rpx 
		rgba(255, 255, 255, 0.5);
}
.pickShow {
	width: 575rpx;
	height: 72rpx;
	background-image: linear-gradient(
		rgba(164, 165, 169, 0.91), 
		rgba(164, 165, 169, 0.91)), 
	linear-gradient(
		#eeeeee, 
		#eeeeee);
	background-blend-mode: normal, 
		normal;
	box-shadow: 0rpx 6rpx 0rpx 0rpx 
		#81807d, 
		0px -1rpx 0rpx 0rpx 
		rgba(255, 255, 255, 0.5);
	border-radius: 36rpx;
	text-align: center;
	margin: auto;
	font-size: 34rpx;
	color: #FFFFFF;
	letter-spacing: 3rpx;
	line-height: 72rpx;
	margin-bottom: 30rpx;
	font-weight: 600;
}

.myFriend{
	position: relative;
	width: 720rpx;
	margin: auto;
	display: flex;
}
.xian {
	width: 30%;
	height: 0.5rpx;
	border-top: 1rpx solid #FFFFFF;
	position: relative;
	top: 25rpx;
}
.friendTitle {
	width: 60%;
	font-size: 34rpx;
	color: #ffc62f;
	letter-spacing: 3rpx;
	text-align: center;
	padding: 0 20rpx 0 20rpx;
	box-sizing: border-box;
}
.frendShow{
	font-size: 25rpx;
	letter-spacing: 3rpx;
	color: #ffc62f;
}
.frendList{
	display: flex;
	flex-wrap: wrap;
	width: 720rpx;
	margin: auto;
	
}
.user {
	width: 25%;
	color: #ffc62f;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.user image {
	width: 50rpx;
	height: 50rpx;
	border-radius: 50%;
	position: relative;
	top: 10rpx;
}
.userName{
	font-size: 25rpx;
	color: #ffc62f;
	letter-spacing: 3rpx;
	line-height: 30rpx;
	position: relative;
	top: -5rpx;
	margin-left: 10rpx;
}
.myName {
	font-weight: 600;
}
.more {
	text-align: center;
	font-family: PingFang-SC-Medium;
	font-size: 22rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 45rpx;
	letter-spacing: 2rpx;
	color: #ffcb00;
	margin-top: 30rpx;
	margin-bottom: 30rpx;
}
.rule {
	width: 657rpx;
	height: 1047rpx;
	padding-bottom: 10rpx;
	box-sizing: border-box;
	margin: auto;
	margin-top: 30rpx;
}
.rule image {
	width: 100%;
	height: 100%;
}
.rule-title {
	width: 300rpx;
	height: 76rpx;
	display: flex;
	font-size: 40rpx;
	color: #FFFFFF;
	letter-spacing: 4rpx;
	font-weight: 600;
	margin: auto;
	text-align: center;
}
.rule-name {
	margin: 0 15rpx 0 20rpx;
}
.rule-title image {
	width: 39rpx;
	height: 30rpx;
	margin-top: 18rpx;
}
.bottomMa{
	width: 100%;
	height: 160rpx;
	background: url(data:image/png;base64,);
	background-size:100% ;
	background-position: center center;
	background-repeat: no-repeat;
	position: absolute;
	bottom: 0rpx;
	left: 0;
}
.weis{
	color: #fff413;
}

 
